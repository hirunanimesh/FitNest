{
  "name": "fitnest-migrations",
  "version": "1.0.0",
  "description": "Migration management for FitNest application",
  "scripts": {
    "migration:new": "npx supabase migration new",
    "migration:list": "npx supabase migration list",
    "migration:link": "npx supabase link",
    "migration:push": "npx supabase db push",
    "migration:reset": "npx supabase db reset",
    "types:generate": "npx supabase gen types typescript --local",
    "test": "jest --selectProjects \"API Gateway Tests\" \"AuthService Tests\" \"UserService Tests\" \"GymService Tests\" \"PaymentService Tests\" \"AdminService Tests\"",
    "test:watch": "jest --watch --selectProjects \"API Gateway Tests\" \"AuthService Tests\" \"UserService Tests\" \"GymService Tests\" \"PaymentService Tests\" \"AdminService Tests\"",
    "test:coverage": "jest --coverage --selectProjects \"API Gateway Tests\" \"AuthService Tests\" \"UserService Tests\" \"GymService Tests\" \"PaymentService Tests\" \"AdminService Tests\"",
    "test:user": "jest --selectProjects \"UserService Tests\"",
    "test:auth": "jest --selectProjects \"AuthService Tests\"",
    "test:trainer": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config backend/TrainerService/jest.config.cjs --colors -i",
    "test:all": "npm run test; npm run test:trainer",
    "test:user:api": "jest --selectProjects \"UserService Tests\" --testPathPatterns=\"backend/UserService/__tests__/(user\\.routes\\.int\\.test\\.js|calendar\\.controller\\.test\\.js)$\"",
    "test:auth:api": "jest --selectProjects \"AuthService Tests\" --testPathPatterns=\"backend/AuthService/__tests__/(auth\\.http\\.int\\.unit\\.test\\.js|authRoutes\\.int\\.unit\\.test\\.js)$\"",
    "test:trainer:api": "npm --prefix backend/TrainerService test -- --testPathPatterns=\"trainer\\.http\\.int\\.test\\.js$\"",
    "test:gym:api": "npm --prefix backend/GymService test -- --testPathPatterns=\"(gym|plans)\\.http\\.int\\.test\\.js$\"",
    "test:payment:api": "npm --prefix backend/PaymentService test -- --testPathPatterns=\"payment\\.http\\.int\\.test\\.js$\"",
    "test:admin:api": "npm --prefix backend/AdminService test -- --testPathPatterns=\"admin\\.http\\.int\\.test\\.js$\""
  },
  "keywords": [
    "supabase",
    "migrations",
    "database"
  ],
  "author": "FitNest Team",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.12",
    "@types/react-big-calendar": "^1.16.2",
    "babel-jest": "^30.1.2",
    "jest": "^30.1.3",
    "nock": "^14.0.10",
    "supabase": "^2.34.3",
    "supertest": "^7.1.4"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.19",
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/interaction": "^6.1.19",
    "@fullcalendar/react": "^6.1.19",
    "@fullcalendar/timegrid": "^6.1.19",
    "date-fns": "^4.1.0",
    "moment": "^2.30.1"
  }
}
